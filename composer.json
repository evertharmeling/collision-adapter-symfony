{
    "name": "nunomaduro/collision-adapter-symfony",
    "description": "Collision's adapter for Symfony applications. Error Reporting for console/command-line PHP applications.",
    "keywords": ["symfony", "bundle", "console", "command-line", "php", "cli", "error", "handling", "collision", "laravel-zero"],
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Nuno Maduro",
            "email": "enunomaduro@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "nunomaduro/collision": ">=3.0",
        "symfony/framework-bundle": ">=5.3"
    },
    "require-dev": {
        "phpstan/phpstan": ">=0.12",
        "phpunit/phpunit": ">=9.6",
        "symfony/filesystem": "*",
        "symfony/yaml": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "NunoMaduro\\CollisionAdapterSymfony\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
